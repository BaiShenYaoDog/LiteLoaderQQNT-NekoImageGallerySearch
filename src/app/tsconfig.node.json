{
  "extends": "@tsconfig/node20/tsconfig.json",
  "include": ["vite.config.*",
    "services/bridge/*.ts", "**/*.ts",
    "components/search/imageSearchInput.vue",
    "../common/share/*.ts"
  ],
  "exclude": ["appEntry.ts"],
  "compilerOptions": {
    "composite": true,
    "noEmit": true,
    "tsBuildInfoFile": "../../node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "baseUrl": ".",
    "lib": ["dom", "es6"],
    "types": ["node"]
  }
}
