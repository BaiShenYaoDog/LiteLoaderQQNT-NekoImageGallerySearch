{
  "manifest_version": 4,
  "type": "extension",
  "name": "NekoImageGallerySearch",
  "slug": "image_search",
  "description": "Another web UI for NekoImageGallery, seamlessly integrated with NTQQ through LiteLoaderQQNT",
  "version": "0.1.0",
  "icon": "./icon.png",
  "authors": [
    {
      "name": "pk5ls20",
      "link": "https://github.com/pk5ls20"
    }
  ],
  "platform": [
    "win32",
    "linux",
    "darwin"
  ],
  "injects": {
    "renderer": "./renderer/renderer.mjs",
    "main": "./main/main.js",
    "preload": "./preload/preload.js"
  },
  "repository": {
    "repo": "pk5ls20/LiteLoaderQQNT-NekoImageGallerySearch",
    "branch": "main",
    "release": {
      "tag": "v0.1.0",
      "file": "LiteLoaderQQNT-NekoImage.zip"
    }
  }
}
